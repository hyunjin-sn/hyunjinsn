// 조회
this.fn_search = function (brlNo)
{
	var Arg_1 = brlNo;
	trace(Arg_1)
	if (this.gfn_isNull(Arg_1))
	{
		Arg_1 = this.ds_list_1.getColumn(this.ds_list_1.rowposition,"BRL_NO");
	}
	
	var id = "ES003_S01";
	var url = "svcUrl::ES003_S01";
	var inDs = "";
	var outDs = "ds_list_1=ds_list_1 ds_list_2=ds_list_2 ds_list_3=ds_list_3 ds_list_4=ds_list_4 ds_list_5=ds_list_5";
	var args = "BRL_NO="+Arg_1+"";
	var callBackid = "fn_afterSearch";
		
	this.gfn_transaction( id, url, inDs, outDs, args, callBackid);
};

this.fn_afterSearch = function (strSvcID, nErrorCode, strErrorMag) {
	if (nErrorCode < 0) 
	{
		this.alert(strErrorMag)
		return ;
	}

    var religion = this.ds_list_1.getColumn(this.ds_list_1.rowposition,"RELIGION");
	
	if (!this.gfn_isNull(religion))
	{
		this.Div06.form.div_mCombo8.form.set_value(religion);
	}
	
	// 시설구분
	this.fn_CheckboxSetting();
}

// 신규입력
this.fn_add = function ()
{
	this.ds_list_1.addRow();
	
	this.ds_list_2.clearData();
	this.ds_list_3.clearData();
	this.ds_list_4.clearData();
	this.ds_list_5.clearData();
	
	this.fn_CheckboxSetting();
};

this.fn_save = function ()
{
	if(this.fn_valChk() == false)
	{
		return;
	}
	
	this.ds_list_2.filter("");
	this.ds_list_3.filter("");
	this.ds_list_4.filter("");
	this.ds_list_5.filter("");
	
	var regUser = this.objApp.gds_login_info.getColumn(0,"USR_ID");
	
	var id = "ES003_M01";
	var url = "svcUrl::ES003_M01";
	var inDs = "ds_list_1=ds_list_1:U";
	var outDs = "ds_list_out=ds_list_out";
	var args = "INSRT_USER_ID="+regUser+"";
	var callBackid = "fn_detailSave";
		
	this.gfn_transaction( id, url, inDs, outDs, args, callBackid);
};

// 디테일 저장
this.fn_detailSave = function (strSvcID, nErrorCode, strErrorMag) {
	if (nErrorCode < 0) 
	{
		this.alert(strErrorMag)
		return ;
	}
	
	var regUser = this.objApp.gds_login_info.getColumn(0,"USR_ID");
	var brlNo = this.ds_list_1.getColumn(this.ds_list_1.rowposition,"BRL_NO");

	if (this.gfn_isNull(brlNo))
	{
		brlNo = this.ds_list_out.getColumn(this.ds_list_out.rowposition,"BRL_NO")
	}	

	var id = "ES003_M02";
	var url = "svcUrl::ES003_M02";
	var inDs = "ds_list_2=ds_list_2:U ds_list_3=ds_list_3:U ds_list_4=ds_list_4:U ds_list_5=ds_list_5:U";
	var outDs = "ds_list_out=ds_list_out";
	var args = "INSRT_USER_ID="+regUser+" BRL_NO="+brlNo+"";
	var callBackid = "fn_afterSave";
		
	this.gfn_transaction( id, url, inDs, outDs, args, callBackid);
}

// 저장 callback
this.fn_afterSave = function (strSvcID, nErrorCode, strErrorMag) {
	if (nErrorCode < 0) 
	{
		this.alert(strErrorMag)
		return ;
	}
	
	this.alert("저장 되었습니다.");
	
	var brlNo = this.ds_list_out.getColumn(0,"BRL_NO");
	
	this.fn_search(brlNo);
}

this.fn_delete = function ()
{
	this.ds_list_1.deleteRow(this.ds_list_1.rowposition);
	this.ds_list_2.deleteAll();
	this.ds_list_3.deleteAll();
	this.ds_list_4.deleteAll();
	this.ds_list_5.deleteAll();
	
	var regUser = this.objApp.gds_login_info.getColumn(0,"USR_ID");
	

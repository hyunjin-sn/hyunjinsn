// 콤보 세팅
this.fn_setCombo = function (obj)
{
	this.efn_SetDatasetData2(this.ds_manage,this.objApp.gds_code2,'ES016');				// 운영형태
	this.efn_SetDatasetData2(this.ds_size,this.objApp.gds_code2,'ES006');				// 시설규모
	this.efn_SetDatasetData2(this.ds_condition,this.objApp.gds_code2,'ES003');			// 시설상태

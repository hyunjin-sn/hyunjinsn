			var inDs = "ds_list_1=ds_list_1:U ds_list_2=ds_list_2:U ds_list_3=ds_list_3:U ds_list_4=ds_list_4:U ds_list_5=ds_list_5:U";
			var outDs = "";
			var args = "INSRT_USER_ID="+Arg_1+"";
			var callBackid = "fn_aftersave2";
				
			this.gfn_transaction( id, url, inDs, outDs, args, callBackid);			
		} else
		{
			this.Div00.form.CheckBox00.set_value(false);
			return;
		}		
	} else
	{
		if (this.confirm("마감 취소 하시겠습니까?"))
		{
			this.ds_list_1.setColumn(this.ds_list_1.rowposition,"CFM_YN","0");
			this.ds_list_1.setColumn(this.ds_list_1.rowposition,"CFM_STATUS","");
			
			var Arg_1 = this.objApp.gds_login_info.getColumn(0,"USR_ID");
	
			var id = "TM004_M01";
			var url = "svcUrl::TM004_M01";
			var inDs = "ds_list_1=ds_list_1:U ds_list_2=ds_list_2:U ds_list_3=ds_list_3:U ds_list_4=ds_list_4:U ds_list_5=ds_list_5:U";
			var outDs = "";
			var args = "INSRT_USER_ID="+Arg_1+"";
			var callBackid = "fn_aftersave2";
				
			this.gfn_transaction( id, url, inDs, outDs, args, callBackid);
		} else
		{
			this.Div00.form.CheckBox00.set_value(true);
			return;
		}		
	}
};


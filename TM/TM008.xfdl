	var saleAmt3 = this.gfn_nvl(this.ds_list_4.getSum("AMT"),0);   															//대체 매출
	var purchaseAmt3 = this.gfn_nvl(this.ds_list_4.getSum("PURCHASE_AMT"),0);   											//대체 매입		
	var addProfit2 = this.gfn_nvl(this.ds_list_4.getSum("PO_PROFIT"),0);													//대체 이익금
// 	var baseAmt = this.gfn_nvl(this.ds_list_1.getColumn(this.ds_list_1.rowposition,"BASE_PKG_AMT"),0);
// 	var addAmt = this.gfn_nvl(this.ds_list_1.getColumn(this.ds_list_1.rowposition,"ADD_PKG_AMT"),0);
// 	var chgAmt = this.gfn_nvl(this.ds_list_1.getColumn(this.ds_list_1.rowposition,"CHG_PKG_AMT"),0);
	var rate = this.gfn_nvl(this.ds_list_1.getColumn(this.ds_list_1.rowposition,"PO_RATE"),0);
	
	// 카드결제액
	this.ds_list_5.filter("PAY_TYPE=='C1' || PAY_TYPE=='CX'")
	cardAmt = this.gfn_nvl(this.ds_list_5.getSum("BILL_AMT"),0);
	this.ds_list_5.filter("")
	
	var cashAmt = 0;
	// 현금결제액
	this.ds_list_5.filter("PAY_TYPE=='C2' || PAY_TYPE=='CY'")
	cashAmt = this.gfn_nvl(this.ds_list_5.getSum("BILL_AMT"),0);
	this.ds_list_5.filter("")	
	
	// 추가 RT
	this.ds_list_3.filter("GUBUN=='FS0082'")
	rt = this.gfn_nvl(this.ds_list_3.getSum("AMT2"),0);
	this.ds_list_3.filter("")
	
	// 기본 기타매출
	this.ds_list_2.filter("GUBUN=='FS0085'")
	//this.Div00.form.Tab00.Tabpage1.form.MaskEdit00_01.set_value(this.gfn_nvl(this.ds_list_2.getSum("PURCHASE_AMT"),0));
	etcAmt = this.gfn_nvl(this.ds_list_2.getSum("AMT"),0);
	this.ds_list_2.filter("")
	
	// 추가 기타원가
	this.ds_list_3.filter("GUBUN=='FS0084'")
	//this.Div00.form.Tab00.Tabpage1.form.MaskEdit00_01.set_value(this.gfn_nvl(this.ds_list_2.getSum("PURCHASE_AMT"),0));
	etcPurAmt = this.gfn_nvl(this.ds_list_3.getSum("PURCHASE_AMT2"),0);
	this.ds_list_3.filter("")
	
	// 본사이익조정
	this.ds_list_3.filter("GUBUN=='FS0086'")
	//this.Div00.form.Tab00.Tabpage1.form.MaskEdit00_01.set_value(this.gfn_nvl(this.ds_list_2.getSum("PURCHASE_AMT"),0));
	chgPurAmt = this.gfn_nvl(this.ds_list_3.getSum("PURCHASE_AMT2"),0);
	this.ds_list_3.filter("")

	// 기본 공제
	this.ds_list_2.filter("GUBUN=='FS0054'")
	//this.Div00.form.Tab00.Tabpage1.form.MaskEdit00_01.set_value(this.gfn_nvl(this.ds_list_2.getSum("PURCHASE_AMT"),0));
	ddtAmt = this.gfn_nvl(this.ds_list_2.getSum("AMT"),0);
	this.ds_list_2.filter("")
	
	// 기본 매출
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"BASE_PKG_AMT",baseAmt+ddtAmt+etcAmt);
	
	saleAmt = this.gfn_nvl(this.ds_list_1.getColumn(this.ds_list_1.rowposition,"BASE_PKG_AMT"),0);
	
	// 기본 공제
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"DDT_PKG_AMT",ddtAmt);
	
	// 기본 매입합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"BASE_PKG_PUR_AMT",purchaseAmt);
	
	// 기본 본부 이익금
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"BASE_PKG_PO_PROFIT",(saleAmt-purchaseAmt)*this.gfn_nvl(rate,0)/100);

	//기본 본사 이익금
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"BASE_PKG_PROFIT",(saleAmt-purchaseAmt)*(100-this.gfn_nvl(rate,0))/100);
	
	// 추가 RT
	this.Div00.form.Tab00.Tabpage2.form.MaskEdit00_01.set_value(rt);
	
	// 추가 매출합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"ADD_PKG_AMT",saleAmt2-rt);
	
	// 추가 매입합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"ADD_PKG_PUR_AMT",purchaseAmt2-etcPurAmt-chgPurAmt);
	
	// 추가 본부 이익금
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"ADD_PKG_PO_PROFIT",addProfit);
	
	//추가 본사 이익금
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"ADD_PKG_PROFIT",saleAmt2-purchaseAmt2-addProfit);
	
	// 대체 매출합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"CHG_PKG_AMT",saleAmt3);
	
	// 대체 매입합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"CHG_PKG_PUR_AMT",purchaseAmt3);
	
	// 대체 본부 이익금
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"CHG_PKG_PO_PROFIT",addProfit2);
	
	//대체 본사 이익금
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"CHG_PKG_PROFIT",saleAmt3-purchaseAmt3-addProfit2);
	
	// 매출 합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"TOTAL_AMT2",saleAmt+saleAmt2+saleAmt3-rt);
	
	// 매입 합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"TOTAL_PUR",purchaseAmt+purchaseAmt2+purchaseAmt3-etcPurAmt-chgPurAmt);
	
	// 본부 합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"TOTAL_PO_PROFIT",((saleAmt-purchaseAmt)*this.gfn_nvl(rate,0)/100)+addProfit+addProfit2);
	
	// 본사 합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"TOTAL_PROFIT",((saleAmt-purchaseAmt)*(100-this.gfn_nvl(rate,0))/100)+(saleAmt2-purchaseAmt2-addProfit)+(saleAmt3-purchaseAmt3-addProfit2));
	
	// 카드 합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"CARD_AMT",cardAmt);
	
	// 현금 합계
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"CASH_AMT",cashAmt);
	
	//본부 수령액
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"TOTAL_PO_GET",(purchaseAmt+purchaseAmt2+purchaseAmt3-etcPurAmt-chgPurAmt-rt)+(((saleAmt-purchaseAmt)*this.gfn_nvl(rate,0)/100)+addProfit+addProfit2)-cashAmt);
	
	//본부 정산액
	this.ds_list_1.setColumn(this.ds_list_1.rowposition,"TOTAL_PO_AMT",(purchaseAmt+purchaseAmt2+purchaseAmt3-etcPurAmt-chgPurAmt-rt)+(((saleAmt-purchaseAmt)*this.gfn_nvl(rate,0)/100)+addProfit+addProfit2));
};


{	
	if (this.ds_list_1.getColumn(this.ds_list_1.rowposition,"PO_CFM_STATUS") != "마감")
	{
		this.alert("본부 마감이 되어 있어야 본사 마감할 수 있습니다.");
		this.Div00.form.CheckBox00.set_value(false);
		return;
	}
	
	if (this.Div00.form.CheckBox00.value == true)
	{
		if (this.confirm("마감 처리 하시겠습니까?"))
		{
			this.ds_list_1.setColumn(this.ds_list_1.rowposition,"CFM_YN","1");
			this.ds_list_1.setColumn(this.ds_list_1.rowposition,"CFM_STATUS","마감");
			
			var Arg_1 = this.objApp.gds_login_info.getColumn(0,"USR_ID");
	
			var id = "TM004_M01";
